{
  "name": "sunburst-chart",
  "version": "0.9.2",
  "description": "Sunburst interactive chart web component for visualizing hierarchical data",
  "main": "dist/sunburst-chart.min.js",
  "module": "dist/sunburst-chart.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/sunburst-chart.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "sunburst",
    "hierarchical",
    "graph",
    "svg"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/sunburst-chart/issues"
  },
  "homepage": "https://github.com/vasturiano/sunburst-chart",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c --banner \"$(preamble)\"",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/sunburst-chart.js -o dist/sunburst-chart.min.js -c -m --preamble \"$(preamble)\"",
    "prepublish": "npm run build && npm run minify"
  },
  "dependencies": {
    "d3-selection": "^1.1.0",
    "kapsule": "^1.7.7"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "package-preamble": "~0.0.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^3.0.28"
  }
}
